    <h1 class="current-sem"><%= params[:sem] %></h1>
<% if @labs.count == 0 %>
    <h3>There are no labs currently in this sem</h3>
<% else %>
   <% @labs.each do |lab| %>
     <h3><%= link_to lab.name, lab_path(lab) %></h3>
   <% end %>
<% end %>