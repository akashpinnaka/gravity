<style type="text/css">
 h1 {
   color: #000;
   text-align: center;
   text-decoration: underline;
 }
 h3 {
 	text-align: center;
 }
  .site-footer {

    position: fixed;
    bottom: 0px;
    left: 0px;
    right: 0px;
    margin-bottom: 0px;

}
